<?php
/**
 * @file
 * Contains unit tests for the WIM profile.
 */

/**
 * Tests the validation of the number portability information.
 */
class WIMExampleTestCase extends DrupalUnitTestCase {

  /**
   * Necessary information for Drupal to detect this test.
   */
  public static function getInfo() {
    return array(
      'name' => 'Example test',
      'description' => 'Simple example test.',
      'group' => 'WIM',
    );
  }

  /**
   * Set-up the test and include the necessary file(s).
   */
  public function setUp() {
    parent::setUp();
  }

  /**
   * Just a simple test.
   */
  public function testAddingDigits() {
    $this->assertTrue(1 + 1 == 2, '1+1 is indeed 2');
    $this->assertFalse(1 + 1 == 2, '1+1 is not 2');
  }

}
